[alias]
  br = branch
  co = checkout
  df = diff -w
  dff = diff --staged
  lg = log --graph  --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=local
  put = push origin HEAD -u
  st = status
  c = commit
